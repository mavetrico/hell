$ apt update && apt upgrade
$ apt install git
$ cd termux-ubuntu
$ git clone https://github.com/Neo-Oli/termux-ubuntu
$ chmod +x ubuntu.sh
$ sh ubuntu.sh
./start-ubuntu.sh
